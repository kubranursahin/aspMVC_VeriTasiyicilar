@using aspMVC2.Models
@model List<User>
@{
    ViewBag.Title = "FilterByRole";
}

<h2>FilterByRole</h2>
    <div class=" alert alert-info"> @ViewBag.Message </div>
    <div class="alert alert-danger"> @ViewData["VDMessage"] </div>
    <div class="alert alert-warning"> @ViewBag.Count </div>
<table class=" table table-bordered table-striped">
    <tr>
        <th> Name </th>
        <th> Email </th>
        <th> Address </th>
        <th> </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.FullName</td>
            <td>@item.Email</td>
            <td>@item.Address</td>
            <td> </td>
        </tr>
    }
</table>

